{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "OpenClaw Windows Node Client",
  "version": "0.1.0",
  "identifier": "ai.openclaw.windows.nodeclient",
  "build": {
    "frontendDist": "../web"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "OpenClaw Windows Node Client",
        "width": 920,
        "height": 700,
        "resizable": true,
        "visible": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": false
  }
}
